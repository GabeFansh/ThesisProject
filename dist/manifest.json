{
    "version": "1.0.0",
    "manifest_version": 3,
    "name": "Study Safari",
    "description": "This is a chrome extension for studying",
    "action": {
        "default_popup": "js/index.html",
        "default_title": "React Chrome Extension"
    },
    "permissions": [
        "storage",
        "tabs",
        "cookies",
        "webNavigation",
        "webRequest",
        "activeTab",
        "declarativeContent"
    ],
    "host_permissions": [
        "https://www.example.com/*",
        "*://*/*"
    ],
    "background": {
    "service_worker": "background.js"
    }
}